	  if(BrowserDetect.OS!="Windows" && BrowserDetect.OS==obj.osObject[i].name && obj.osObject[i].mEnabledOs){    	   
		document.write("<div id=\"detected\">"+_format('The following system was detected:',deviceConfigId,actionId)+"</div>");
		if(obj.osObject[i].mLogo != null && obj.osObject[i].mLogo != undefined){
			var imgPath = obj.osObject[i].mLogo.mPath;
			document.write("<div id=\"oslogo\"><img src=\"images/"+obj.osObject[i].name.replace(/[^\w]/gi, '_')+imgPath.substring(imgPath.lastIndexOf(".")
			,imgPath.length)+"\"/></div><br/>");
		}
		if(obj.osObject[i].mShowTitle){		
			document.write("<h2>"+BrowserDetect.OS+"</h2>");
		}
		
		diplaySelfConfiguration(obj.osObject[i]);
		if(!selectedManually){
			$.post("php/deploy.php", { operatingSystem:BrowserDetect.OS}).done(function(data) {
			});
		}
		vpnOSFound = true;
	    break;
	  }
	}
	if(!vpnOSFound){
        var vpnWindowsOperatingSystem = getOperatingSystemObject("Windows",obj.osObject);
	   
	   document.write("<div id=\"detected\">"+_format('The following system was detected:',deviceConfigId,actionId)+"</div>");
		var imgPathVpnWindows = vpnWindowsOperatingSystem.mLogo.mPath;
	    
        document.write("<div id=\"oslogo\"><img src=\"images/windows"+imgPathVpnWindows.substring(imgPathVpnWindows.lastIndexOf(".")
			,imgPathVpnWindows.length)+"\" alt=\"windows\"/></div>");
	    
		if(vpnWindowsOperatingSystem.mShowTitle){		
			document.write("<h2>"+BrowserDetect.OS+"</h2>");
		}		
	    diplaySelfConfiguration(vpnWindowsOperatingSystem);
 }
